// portfolio section start
section.rtp_portfolio#rtp_s__portfolio
	.rtp_container
		h2.rtp_section__title Portfolio
		h5.rtp_sTitle Lorem Ipsum - це текст-"риба", що використовується в друкарстві та дизайні. Lorem Ipsum є, фактично, стандартною "рибою" аж з XVI сторіччя.
		.rtp_row
			.col.col--12-of-12
				// portfolio navigation
				nav.rtp_portfolio__nav
					ul.rtp_portfolio__list
						li(data-filter="all") All
						li(data-filter=".web") Web
						li(data-filter=".photog") Photography
						li(data-filter=".design") Design
			.col.col--12-of-12
				.rtp_portfolio__items
					// portfolio item
					.col.col--1-of-3.mix.web
						.rtp_portfolio__item
							a(href="assets/img/portfolio_images/3vqmhzqpdiy-austin-neill.jpg" class="portfolio_popup").rtp_image__holder
								img(src="assets/img/portfolio_images/3vqmhzqpdiy-austin-neill.jpg", alt="image")
